install.packages('forecast')
library('forecast')
class(AirPassengers)
my_colors <- rainbow(12)
boxplot(split(AirPassengers, cycle(AirPassengers)),
 xlab = "Month", ylab= "Number of Passangers",
 col = my_colors,
 border = "black",
 main = "Monthly Air Passanger Counts by Month",
 names = month.abb,
 outline = FALSE)
model <- auto.arima(AirPassengers)
summary(model)
f<-forecast(model,level=c(95), h=10*12)
plot(f)
